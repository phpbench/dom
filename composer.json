{
    "name": "phpbench/dom",
    "description": "DOM wrapper to simplify working with the PHP DOM implementation",
    "license": "MIT",
    "authors": [
        {
            "name": "Daniel Leech",
            "email": "daniel@dantleech.com"
        }
    ],
    "require": {
        "php": "^7.3||^8.0",
        "ext-dom": "*"
    },
    "require-dev": {
        "friendsofphp/php-cs-fixer": "^3.14",
        "phpunit/phpunit": "^8.0||^9.0",
        "phpstan/phpstan": "^1.10"
    },
    "autoload": {
        "psr-4": {
            "PhpBench\\Dom\\": "lib/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "PhpBench\\Dom\\Tests\\": "tests/"
        }
    },
    "extra": {
        "branch-alias": {
            "dev-master": "1.0-dev"
        }
    }
}
